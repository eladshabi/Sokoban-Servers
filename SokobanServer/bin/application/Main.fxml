<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.VBox?>

<BorderPane xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.MainWindowController">
   <right>
      <VBox prefHeight="200.0" prefWidth="100.0" BorderPane.alignment="CENTER">
         <children>
            <Label fx:id="label1" prefHeight="40.0" prefWidth="100.0" text="" />
            <Label fx:id="label2" prefHeight="40.0" prefWidth="100.0" text="" />
            <Label fx:id="label3" prefHeight="40.0" prefWidth="100.0" text="" />
            <Label fx:id="label4" prefHeight="40.0" prefWidth="100.0" text="" />
         </children>
      </VBox>
   </right>
   <left>
      <VBox prefHeight="200.0" prefWidth="100.0" BorderPane.alignment="CENTER">
         <children>
            <Button mnemonicParsing="false" onAction="#start_server" prefHeight="50.0" prefWidth="90.0" text="start server" />
            <Button mnemonicParsing="false" onAction="#stop_server" prefHeight="50.0" prefWidth="90.0" text="stop server" />
         </children>
      </VBox>
   </left>
</BorderPane>
